<%= form_for [:admin, @tag] do |f| %>
  <div class="form-group">
    <label for="">Name</label>
    <%= f.text_field :name, class: "form-control", autofocus: true %>
  </div>

  <div class="form-group">
    <%= f.submit "Save", class: "btn btn-default" %>
    <%= link_to "Cancel", admin_tags_path %>
  </div>
<% end %>

